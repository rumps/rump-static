{
  "license": "MIT",
  "main": "./lib",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "preci": "npm run lint",
    "ci": "babel-node node_modules/.bin/isparta cover --report lcovonly _mocha",
    "postci": "codecov < coverage/lcov.info",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib",
    "coverage": "babel-node node_modules/.bin/isparta cover --report html --report text _mocha -- -R min",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "mocha"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "extend": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "codecov.io": "^0.1.5",
    "eslint": "^1.0.0",
    "gulp": "^3.9.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "trash": "^2.0.0"
  }
}
